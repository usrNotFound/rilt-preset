{
    "name": "usrnotfound/rilt-preset",
    "license": "MIT",
    "keywords": [
        "rilt",
        "laravel tailwind preset",
        "laravel frontend preset",
        "laravel react preset",
        "laravel inertiajs preset"
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/console": "^7.0|^8.0",
        "illuminate/filesystem": "^7.0|^8.0",
        "illuminate/support": "^7.0|^8.0",
        "ext-json": "*"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "UsrNotFound\\Ui\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "UsrNotFound\\Ui\\UiServiceProvider"
            ]
        }
    },
    "prefer-stable": true
}
